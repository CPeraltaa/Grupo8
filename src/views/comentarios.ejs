    <div class="container mt-5">
      <div class="row">
        <div class="col-md-7">
          <table class="table table-bordered table-hover">
            <thead>
              <tr>
                <td>no</td>
                <td>Cliente</td>
                <td>Comentario</td>
              </tr>
            </thead>
            <tbody>
              <% if (data) { %>
                <% for(var i = 0; i < data.length; i++) { %>
                   <% if (data[i].status==0) { estado="pendiente";} else {estado="completado";} %>
                  <tr>
                    <td><%= (i + 1) %></td>
                    <td><%= data[i].nombres +' '+ data[i].apellidos %></td>
                    <td><%= data[i].comentario %></td>
                  </tr>
                <% } %>
              <% } %>
            </tbody>
          </table>
        </div>
    
      </div>
    </div>

